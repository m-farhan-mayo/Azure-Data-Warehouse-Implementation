		{
			"name": "[concat(parameters('workspaceName'), '/Transformation Layer')]",
			"type": "Microsoft.Synapse/workspaces/pipelines",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"activities": [
					{
						"name": "Data Transformation",
