		{
			"name": "[concat(parameters('workspaceName'), '/Dataflow')]",
			"type": "Microsoft.Synapse/workspaces/dataflows",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "DataFlow_DS",
								"type": "DatasetReference"
							},
							"name": "source"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "FlowSink_DS",
								"type": "DatasetReference"
							},
							"name": "SilverDestination"
						}
					],
					"transformations": [
						{
							"name": "filter100"
						},
						{
							"name": "cast"
						},
						{
							"name": "AddColumn"
						}
					],
					"scriptLines": [
						"source(output(",
						"          OrderID as string,",
						"          OrderDate as date,",
						"          CustomerID as string,",
						"          CustomerName as string,",
						"          CustomerEmail as string,",
						"          ProductID as string,",
						"          ProductName as string,",
						"          ProductCategory as string,",
						"          RegionID as string,",
						"          RegionName as string,",
						"          Country as string,",
						"          Quantity as string,",
						"          UnitPrice as string,",
						"          TotalAmount as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     format: 'parquet') ~> source",
						"cast filter(UnitPrice >= 100) ~> filter100",
						"source cast(output(",
						"          UnitPrice as double",
